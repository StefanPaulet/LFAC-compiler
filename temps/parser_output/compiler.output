Grammar

    0 $accept: N_START $end

    1 N_START: N_DECLARATION T_TYPE T_MAIN_IDENTIFIER '(' ')' N_BLOCK

    2 N_CONSTANT_VALUE: T_INT_NUMBER
    3                 | T_FLOAT_NUMBER
    4                 | T_CHAR_VALUE
    5                 | T_BOOL_VALUE
    6                 | T_STRING_VALUE

    7 N_SYMBOL_ACCESS: N_NON_ARRAY_SYMBOL_ACCESS
    8                | N_ARRAY_SYMBOL_ACCESS

    9 N_NON_ARRAY_SYMBOL_ACCESS: N_SYMBOL_ACCESS M_PARAMETER_LOOKUP_SCOPE_CHANGE '(' N_PARAMETER ')'
   10                          | T_IDENTIFIER

   11 M_PARAMETER_LOOKUP_SCOPE_CHANGE: ε

   12 N_ARRAY_SYMBOL_ACCESS: N_NON_ARRAY_SYMBOL_ACCESS N_ARRAY_SUBSCRIPT

   13 $@1: ε

   14 N_VARIABLE_ACCESS: N_VARIABLE_ACCESS M_CLASS_ACCESS_SCOPE_CHANGE $@1 '.' N_SYMBOL_ACCESS
   15                  | N_SYMBOL_ACCESS

   16 M_CLASS_ACCESS_SCOPE_CHANGE: ε

   17 N_PARAMETER: N_PARAMETER_LIST
   18            | ε

   19 N_PARAMETER_LIST: N_PARAMETER_LIST ',' N_SYMBOL_ACCESS
   20                 | N_SYMBOL_ACCESS

   21 N_MIXED_TYPE: T_TYPE
   22             | T_IDENTIFIER

   23 N_EXPRESSION: N_EXPRESSION '+' N_EXPRESSION
   24             | N_EXPRESSION '-' N_EXPRESSION
   25             | N_EXPRESSION '*' N_EXPRESSION
   26             | N_EXPRESSION '/' N_EXPRESSION
   27             | '(' N_EXPRESSION ')'
   28             | '-' N_EXPRESSION
   29             | N_CONSTANT_VALUE
   30             | N_VARIABLE_ACCESS

   31 N_BOOL_EXPRESSION: N_BOOL_EXPRESSION T_BAND N_BOOL_EXPRESSION
   32                  | N_BOOL_EXPRESSION T_BOR N_BOOL_EXPRESSION
   33                  | '!' N_BOOL_EXPRESSION
   34                  | N_CONSTANT_VALUE
   35                  | N_VARIABLE_ACCESS

   36 N_DECLARATION: N_DECLARATION N_FUNCTION_DECLARATION
   37              | N_DECLARATION N_BLOCK_SCOPE_DECLARATION ';'
   38              | N_DECLARATION error ';'
   39              | ε

   40 N_BLOCK_SCOPE_DECLARATION: N_CLASS_DECLARATION
   41                          | N_VARIABLE_DECLARATION
   42                          | N_VARIABLE_DEFINITION

   43 N_VARIABLE_DECLARATION: N_MIXED_TYPE T_IDENTIFIER
   44                       | N_MIXED_TYPE T_CONST T_IDENTIFIER
   45                       | N_MIXED_TYPE T_IDENTIFIER N_ARRAY_SUBSCRIPT
   46                       | N_MIXED_TYPE T_CONST T_IDENTIFIER N_ARRAY_SUBSCRIPT

   47 N_VARIABLE_DEFINITION: N_MIXED_TYPE T_IDENTIFIER '=' N_EXPRESSION
   48                      | N_MIXED_TYPE T_CONST T_IDENTIFIER '=' N_EXPRESSION

   49 N_ARRAY_SUBSCRIPT: '[' T_INT_NUMBER ']' N_ARRAY_SUBSCRIPT
   50                  | '[' T_INT_NUMBER ']'

   51 N_CLASS_DECLARATION: T_CLASS T_IDENTIFIER '{' M_BLOCK_BEGIN N_DECLARATION '}'

   52 M_BLOCK_BEGIN: ε

   53 $@2: ε

   54 N_FUNCTION_DECLARATION: N_MIXED_TYPE T_IDENTIFIER '(' N_PARAMETER_DECLARATION M_FUNCTION_DECLARATION_BLOCK_BEGIN ')' $@2 N_FUNCTION_BLOCK
   55                       | N_MIXED_TYPE T_IDENTIFIER '(' N_PARAMETER_DECLARATION M_FUNCTION_DECLARATION_BLOCK_BEGIN ')'

   56 M_FUNCTION_DECLARATION_BLOCK_BEGIN: ε

   57 N_FUNCTION_BLOCK: '{' N_STATEMENT '}'

   58 N_PARAMETER_DECLARATION: N_PARAMETER_DECLARATION_LIST
   59                        | ε

   60 N_PARAMETER_DECLARATION_LIST: N_PARAMETER_DECLARATION_LIST ',' N_MIXED_TYPE T_IDENTIFIER
   61                             | N_MIXED_TYPE T_IDENTIFIER

   62 N_BLOCK: '{' M_BLOCK_BEGIN N_STATEMENT '}'

   63 N_STATEMENT: N_STATEMENT N_ASSIGNMENT ';'
   64            | N_STATEMENT N_IF_STATEMENT
   65            | N_STATEMENT N_WHILE_STATEMENT
   66            | N_STATEMENT N_FOR_STATEMENT
   67            | N_STATEMENT N_EXPRESSION ';'
   68            | N_STATEMENT N_BLOCK_SCOPE_DECLARATION ';'
   69            | N_STATEMENT error ';'
   70            | N_STATEMENT N_BLOCK
   71            | ε

   72 N_ASSIGNMENT: N_VARIABLE_ACCESS '=' N_EXPRESSION

   73 N_IF_STATEMENT: T_IF '(' N_BOOL_EXPRESSION ')' N_BLOCK

   74 N_WHILE_STATEMENT: T_WHILE '(' N_BOOL_EXPRESSION ')' N_BLOCK

   75 N_FOR_STATEMENT: T_FOR '(' ',' ',' ')' N_BLOCK


