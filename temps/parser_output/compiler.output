    5                 | T_CHAR_VALUE
    6                 | T_BOOL_VALUE

    7 N_EXPRESSION: N_EXPRESSION '+' N_EXPRESSION
    8             | N_EXPRESSION '-' N_EXPRESSION
    9             | N_EXPRESSION '*' N_EXPRESSION
   10             | N_EXPRESSION '/' N_EXPRESSION
   11             | '(' N_EXPRESSION ')'
   12             | '-' N_EXPRESSION
   13             | N_CONSTANT_VALUE
   14             | T_IDENTIFIER
   15             | T_IDENTIFIER '(' N_PARAMETERS ')'

   16 N_DECLARATION: T_IDENTIFIER '=' N_CONSTANT_VALUE ';' N_DECLARATION
   17              | T_IDENTIFIER '=' N_CONSTANT_VALUE ';'

   18 N_PARAMETERS: N_EXPRESSION ',' N_PARAMETERS
   19             | N_EXPRESSION


